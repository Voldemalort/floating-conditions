{
  "parser": "babel-eslint",
  "root": true,
  "globals": {
    "canvas": true,
    "Hooks": true,
    "loadTexture": true,
    "PIXI": true
  },
  "extends": "airbnb",
  "rules": {
    "no-await-in-loop": 0,
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ],
    "no-restricted-syntax": 0,
    "no-shadow": 0,
    "no-underscore-dangle": 0,
    "strict": 0
  }
}
